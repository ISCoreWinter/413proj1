@{
    ViewBag.Title = "Sign Up Continued";
}
@model _413proj1.Models

<h2>Add Your Information</h2>
<h3>Please fill in the following infomration:</h3>

<div>
    <form asp-action="AddInfo" method="post">
        <div>
            <label asp-for="GroupName">Group Name:</label>
            <input asp-for="GroupName" />
        </div>
        <div>
            <label asp-for="Email">Email:</label>
            <input asp-for="Email" />
        </div>
        <div>
            <label asp-for="PhoneNumber">Phone Number:</label>
            <input asp-for="PhoneNumber" />
        </div>
    </form>
</div>